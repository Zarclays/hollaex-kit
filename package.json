	"scripts": {
		"prettier-commit": "lint-staged",
		"prettier": "./node_modules/prettier/bin-prettier.js --write \"src/**/*.{js,json,css,scss,md}\"",
		"build-css": "node-sass-chokidar src/ -o src/",
		"watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
		"start-js": "react-scripts start",
		"start": "npm-run-all -p watch-css start-js",
		"build": "npm run build-css && react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"deploy-main": "npm run build && aws s3 sync build/ s3://hollaex.com && aws cloudfront create-invalidation --distribution-id E1FPBCJ0EFOPZJ --paths /"
	},
	"description": "Cryptocurrency exchange platfrom",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bitholla/hollaex.git"
	},
	"homepage": "https://hollaex.com",
	"prettier": {
		"tabWidth": 2,
		"useTabs": true,
		"semi": true,
		"singleQuote": true,
		"bracketSpacing": true,
		"arrowParens": "always"
	},
	"lint-staged": {
		"*.{js,json,css,scss,md}": [
