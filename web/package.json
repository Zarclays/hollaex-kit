	"devDependencies": {
		"prettier": "1.16.4",
		"react-scripts": "2.1.8"
	},
	"scripts": {
		"prettier-commit": "lint-staged",
		"prettier": "./node_modules/prettier/bin-prettier.js --write \"src/**/*.{js,json,css,scss,md}\"",
		"build-css": "node-sass-chokidar src/ -o src/",
		"watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
		"start-js": "react-scripts --max_old_space_size=4096 start",
		"start": "npm-run-all -p watch-css start-js",
		"build": "npm run build-css && react-scripts --max_old_space_size=4096 build",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject"
	},
	"description": "Cryptocurrency exchange platfrom",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bitholla/hollaex-kit.git"
	},
	"homepage": "https://bitholla.com",
	"prettier": {
		"tabWidth": 3,
		"useTabs": true,
		"semi": true,
		"singleQuote": true,
		"bracketSpacing": true,
		"arrowParens": "always"
	},
	"lint-staged": {
		"*.{js,json,css,scss,md}": [
			"prettier --write",
			"git add"
		]
	},
	"author": "bitHolla Inc",
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
